<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xzx.xzxms.inquiry.dao.extend.SysProTypeExtendMapper">
  <select id="findById" resultType="com.xzx.xzxms.inquiry.bean.SysProType">
    select *
    FROM sys_pro_type
    where id=#{proTypeId,jdbcType=BIGINT}
    order by time desc
  </select>
</mapper>