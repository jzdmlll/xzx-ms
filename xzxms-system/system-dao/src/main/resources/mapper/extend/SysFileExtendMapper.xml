<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xzx.xzxms.system.dao.extend.SysFileExtendMapper">
    <select id="selectByOtherId" parameterType="java.util.Map" resultType="com.xzx.xzxms.system.bean.SysFile">
        select
          name,
          url,
          type
        from
          sys_file
        where
          other_id = #{otherId,jdbcType=BIGINT}
          <if test="type !=null">
              and type = #{type}
          </if>

    </select>
</mapper>